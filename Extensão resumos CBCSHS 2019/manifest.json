{
  "manifest_version": 3,
  "name": "Extrator Resumos 2019 CBCSHS",
  "description": "Navega pelos links e atualiza JSON CBCSHS 2019",
  "version": "1.2",
  "permissions": ["tabs", "storage", "scripting"],
  "host_permissions": [
    "https://proceedings.science/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
